<div class="modal">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">
					<span aria-hidden="true">&times;</span><span class="sr-only">{{loc 'core.close'}}</span>
				</button>
				<h4 class="modal-title">{{loc 'core.sets'}}</h4>
			</div>
			<div class="modal-body">
				<div class="card-set-filter-view-ctr"></div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-raised btn-dummy" id="applyFilter">
					<span class="glyphicon glyphicon-filter"></span>&nbsp;{{loc 'core.filter01'}}
				</button>	
				<button type="button" class="btn btn-default" data-dismiss="modal">
					<span class="glyphicon glyphicon-remove"></span>&nbsp;{{loc 'core.close'}}
				</button>
			</div>
		</div>
	</div>
</div>